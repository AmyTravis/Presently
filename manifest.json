{
  "name": "Presently",
  "version": "0.2.0",
  "manifest_version": 2,
  "permissions": ["storage"],
  "chrome_url_overrides": {"newtab": "presently.html"},
  "options_page": "presently.html?settings",
  "default_locale": "en",
  "description": "Replaces the new tab page with the time, weather, and forecast."
}